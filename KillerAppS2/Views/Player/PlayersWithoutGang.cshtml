@model PlayerViewModel

@{
    ViewData["Title"] = "PlayersWithoutGang";
}

<h2>PlayersWithoutGang</h2>

<p>
    <a asp-action="Index">All Players</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Player.PlayerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.Username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.PlayerLevel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.Experience)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.SkillPoints)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.Money)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.Income)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.Energy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.RealName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.Country)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Player.City)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Players) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerLevel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Experience)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkillPoints)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Money)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Income)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Energy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RealName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Country)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
